@{
    ViewBag.Title = "LoginVendor";
}

@using (Html.BeginForm())
{
    <div>
        <div>
            @Html.Label("Mobile")
        </div><br />
        <div>
            @Html.TextBox("Mobile", "")
        </div>
    </div>
    <div>
        <div>
            @Html.Label("Password")
        </div><br />
        <div>
            @Html.TextBox("password", "")
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Save" class="btn btn-default" />
        </div>
    </div>
}

